# GRAFANA SETUP
GF_SECURITY_ADMIN_USER=grafana
GF_LOG_MODE=console file
GF_LOG_LEVEL=debug
GF_SERVER_PROTOCOL=https
GF_SERVER_CERT_FILE=/usr/share/grafana/conf/certs/grafana.crt
GF_SERVER_CERT_KEY=/usr/share/grafana/conf/certs/grafana.key