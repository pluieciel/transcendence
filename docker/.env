# POSTGRES SETUP
POSTGRES_PORT=5432
POSTGRES_DB=transcendence
POSTGRES_USER=postgres
POSTGRES_HOST=postgres

# DJANGO SETUP
DJANGO_SUPERUSER_USERNAME=django

# ELASTICSEARCH SETUP
ELASTIC_PORT=9200
ELASTIC_HOST=https://elasticsearch:${ELASTIC_PORT}
